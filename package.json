{
    "name": "GuffLabs",
    "version": "0.1.0",
    "description": "Lightweight JavaScript package for running GGUF language models",
    "main": "dist/gguf.js",
    "module": "dist/gguf.esm.js",
    "browser": "dist/gguf.browser.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist",
      "src",
      "README.md",
      "LICENSE"
    ],
    "scripts": {
      "build": "rollup -c",
      "dev": "rollup -c -w",
      "test": "jest",
      "lint": "eslint src",
      "format": "prettier --write \"src/**/*.js\"",
      "prepare": "npm run build",
      "prepublishOnly": "npm test && npm run lint"
    },
    "keywords": [
      "gguf",
      "llm",
      "ai",
      "language-model",
      "llama",
      "inference",
      "nlp",
      "machine-learning",
      "node",
      "react"
    ],
    "author": "TypingPower",
    "license": "MIT",
    "dependencies": {},
    "peerDependencies": {
      "node-llama-cpp": ">=2.0.0"
    },
    "peerDependenciesMeta": {
      "node-llama-cpp": {
        "optional": true
      }
    },
    "devDependencies": {
      "@babel/core": "^7.23.0",
      "@babel/preset-env": "^7.22.20",
      "@rollup/plugin-babel": "^6.0.3",
      "@rollup/plugin-commonjs": "^25.0.4",
      "@rollup/plugin-json": "^6.0.0",
      "@rollup/plugin-node-resolve": "^15.2.1",
      "@rollup/plugin-terser": "^0.4.3",
      "eslint": "^8.49.0",
      "jest": "^29.7.0",
      "prettier": "^3.0.3",
      "rollup": "^3.29.2"
    },
    "engines": {
      "node": ">=14.0.0"
    }
  }